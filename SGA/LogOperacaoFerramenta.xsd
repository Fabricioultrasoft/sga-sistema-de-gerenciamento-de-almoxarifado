                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT l.nu_seq_log
  ,l.descricao_log
  ,l.matricula_ator
  ,l.dt_log
  ,l.mat_funcionario
  ,fe.cod_ferramenta
  ,fe.nu_serie
  ,fe.dt_aquisicao
  ,fe.dt_desativacao
  ,d.no_ferramenta
  ,fa.no_fabricante
  ,g.no_grupo
